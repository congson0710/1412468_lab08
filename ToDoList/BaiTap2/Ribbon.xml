<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://schemas.microsoft.com/windows/2009/Ribbon">
  <Application.Commands>
    <Command Name="cmdExit" Symbol="ID_BTN_EXIT" LabelTitle="Exit" >
      <Command.LargeImages>
        <Image Id="201">Images/Exit.png</Image>
      </Command.LargeImages>
    </Command>
    <Command Name="cmdAbout" Symbol="ID_BTN_ABOUT" LabelTitle="About" >
      <Command.LargeImages>
        <Image Id="202">Images/About.png</Image>
      </Command.LargeImages>
    </Command>
    <Command Name="cmdWriteFile" Symbol="ID_BTN_WRITE" LabelTitle="Save Size" >
      <Command.LargeImages>
        <Image Id="203">Images/Save.png</Image>
      </Command.LargeImages>
    </Command>
    <Command Name="cmdGroup1" LabelTitle="Help" >
    </Command>
    <Command Name="Tab" LabelTitle="Home" >
    </Command>
  </Application.Commands>
  <Application.Views>
    <Ribbon>
      <Ribbon.Tabs>
        <Tab CommandName="Tab">
          <Tab.ScalingPolicy>
            <ScalingPolicy>
              <ScalingPolicy.IdealSizes>
              <Scale Group="cmdGroup1" Size="Large"/>
            </ScalingPolicy.IdealSizes>
            </ScalingPolicy>
          </Tab.ScalingPolicy>
          <Group  CommandName="cmdGroup1" SizeDefinition="ThreeButtons">
            <Button CommandName="cmdExit" />
            <Button CommandName="cmdAbout"/>
            <Button CommandName="cmdWriteFile"/>
          </Group>
        </Tab>
      </Ribbon.Tabs>
    </Ribbon>
  </Application.Views>
</Application>
		